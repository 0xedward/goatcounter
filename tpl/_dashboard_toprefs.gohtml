<div class="hchart" data-detail="/hchart-detail?kind=topref" data-more="/hchart-more?kind=topref">
	<h2>Top referrers</h2>
	{{template "_dashboard_warn_collect.gohtml" .IsCollected}}
	{{if .Err}}
		<em>Error: {{.Err}}</em>
	{{else}}
		{{horizontal_chart .Context .Stats .TotalUniqueHits 6 true true}}
	{{end}}
</div>
