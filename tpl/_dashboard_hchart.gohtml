{{- $x := horizontal_chart .Context .Stats .TotalUniqueUTC (not .RowsOnly) true -}}
{{- if .RowsOnly -}}
	{{- $x -}}
{{- else -}}
	<div class="hchart" data-widget="{{.ID}}">
		<h2>{{.Header}}</h2>
		{{template "_dashboard_warn_collect.gohtml" .IsCollected}}
		{{if .Err}}
			<em>Error: {{.Err}}</em>
		{{else}}
			{{$x}}
		{{end}}
	</div>
{{- end -}}
